server {
  listen 80;
  server_name app.admin.51ishare.com;
  charset utf-8;

  access_log  /var/log/nginx/ishare.access.log  main;

  location / {
    proxy_pass http://localhost:8519/;
    proxy_set_header            Host $host;
    proxy_set_header            X-real-ip $remote_addr;
    proxy_set_header            X-Forwarded-For $proxy_add_x_forwarded_for;
  }
}
